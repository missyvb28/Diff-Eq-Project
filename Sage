A = matrix([[-3/200,1/80,29/80],[3/1000,-2081/80,0],[3/250, 26, -29/80]])
show(A)
show(A.eigenvalues())
#?s refer to imaginary numbers, are these ok?
show(A.eigenvectors_right())

Iu = matrix([[-1.756*10^-3, 0,0], [0, -1.756*10^-3, 0], [0,0,-1.756*10^-3]])
show(Iu)

#for b(a) = 20
b20 = matrix([[-.001695],[0],[0]])
show(b20)
AIu = A*Iu
AIu_Inv = AIu.inverse
show(AIu_Inv())
